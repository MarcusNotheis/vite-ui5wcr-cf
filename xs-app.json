{
    "authenticationMethod": "none",
    "routes": [
      {
        "source": "^/$",
        "localDir": "dist",
        "csrfProtection": false,
        "cacheControl": "no-cache"
      },
      {
        "source": "^/assets",
        "localDir": "dist",
        "csrfProtection": false,
        "cacheControl": "public, max-age=31536000, must-revalidate"
      },
      {
        "source": "^/",
        "localDir": "dist",
        "csrfProtection": false,
        "cacheControl": "public, max-age=3600, must-revalidate"
      }
    ]
  }